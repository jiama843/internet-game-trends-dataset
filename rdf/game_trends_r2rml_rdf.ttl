@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ex: <http://example.com/ns#>.

ex:GameInfoMap
    rr:logicalTable [ rr:tableName "GameInfo" ];
    rr:subjectMap [
        rr:class ex:Game;
    ];
    rr:predicateObjectMap [
        rr:predicate ex:id;
        rr:objectMap [ rr:column "ID" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:name;
        rr:objectMap [ rr:column "Name" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:developer;
        rr:objectMap [ rr:column "Developer" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:publisher;
        rr:objectMap [ rr:column "Publisher" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:perspective;
        rr:objectMap [ rr:column "Perspective of the game (3rd person, isometric etc.)" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:game_engine;
        rr:objectMap [ rr:column "Game Engine" ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:Genres;
        rr:objectMap [
            rr:parentTriplesMap ex:GenreMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:Theme;
        rr:objectMap [
            rr:parentTriplesMap ex:ThemeMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:GameMode;
        rr:objectMap [
            rr:parentTriplesMap ex:GameModeMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:Rating;
        rr:objectMap [
            rr:parentTriplesMap ex:RatingMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:SteamInfo;
        rr:objectMap [
            rr:parentTriplesMap ex:SteamInfoMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:EpicInfo;
        rr:objectMap [
            rr:parentTriplesMap ex:EpicInfoMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ex:SteamDailyStats;
        rr:objectMap [
            rr:parentTriplesMap ex:SteamDailyStatsMap;
            rr:joinCondition [
                rr:child "id";
                rr:parent "game_id";
            ];
        ];
    ].

